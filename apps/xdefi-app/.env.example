# Copy this file to `.env` and fill in values as needed.
# Server-only secrets for OKX proxy at /api/okx

# Optional: base URL for OKX API; defaults to https://web3.okx.com if unset
# With this base, pass `path` including the version prefix (e.g., /api/v6/dex/aggregator/quote)
OKX_BASE_URL=https://web3.okx.com

# Required: OKX API credentials (do not commit real values)
OKX_ACCESS_KEY=
OKX_SECRET_KEY=
OKX_PASSPHRASE=

# Client-side env (exposed to browser; must start with VITE_)
VITE_WALLETCONNECT_PROJECT_ID=
